#: src/freevial.py:152
#, python-format
msgid ""
"\n"
"Freevial, a trivia platform for use on community events.\n"
"\n"
"You are running version %(version)s, which is part of the «%(series)s» series.\n"
"\n"
"https://launchpad.net/freevial/%(series)s\n"
"\n"
msgstr ""
"\n"
"Freevial, una plataforma de trivia per a actes comunitaris.\n"
"\n"
"Esteu executant la versió %(version)s, que forma part de la sèrie «%(series)s».\n"
"\n"
"https://launchpad.net/freevial/%(series)s\n"
"\n"

#: src/freevial.py:162
msgid "Usage: %prog [options]"
msgstr "Sintaxi: freevial [opcions]"

#: src/freevial.py:168
msgid "debug mode"
msgstr "mode de depuració"

#: src/freevial.py:173
msgid "start in fullscreen mode"
msgstr "inicia el joc en mode protegit"

#: src/freevial.py:178
msgid "start game in locked mode"
msgstr "inicia el joc en mode protegit"

#: src/freevial.py:183
msgid "only load those questions written in the indicated language(s); for example, \"ca, en\""
msgstr "carrega només aquelles preguntes escrites en les llengües indicades; per exemple, \"ca, en\""

#: src/freevial.py:189
msgid "path to the database file / directory you want to append to the default database"
msgstr "ruta de la base de dades a afegir a la base de dades predeterminada"

#: src/freevial.py:195
msgid "(do not use this)"
msgstr "(no utilitzeu això)"

#: src/freevial.py:200
msgid "absolute path to the skin directory"
msgstr "ruta absoluta del directori de l'aparença a utilitzar."

#: src/freevial.py:205
msgid "disable sound"
msgstr "desactiva els sons"

#: src/freevial.py:210
msgid "disable music"
msgstr "desactiva la música"

#: src/freevial.py:215
msgid "disable all sounds and music"
msgstr "desactiva tots els sons i la música"

#: src/freevial.py:220
msgid "disable media questions"
msgstr "desactiva les preguntes multimèdia"

#: src/freevial.py:225
msgid "show the framerate on screen"
msgstr "mostra el nombre d'imatges per segon a la pantalla"

#: src/freevial.py:230
msgid "enable D-Bus support, to interface with external applications"
msgstr "habilita la inferfície D-Bus"

#: src/freevial.py:235
msgid "print information about the loaded database and exit"
msgstr "mostra informació sobre la base de dades carregada i surt"

#: src/freevial.py:240
msgid "load all images and sounds at startup"
msgstr "carrega totes les imatges i sons a l'inici"

#: src/freevial.py:245
msgid "use psyco, if available (this will use more memory)"
msgstr "utilitza psyco, si està disponible (això utilitzarà més memòria)"

#: src/freevial.py:254
msgid "Error: You've indicated an incorrect language code. Valid examples are: \"ca\", \"en, de\", etc."
msgstr "Error: Heu indicat un codi d'idioma invàlid. Utilitzeu valors com ara \"ca\", \"en, de\", etc."

